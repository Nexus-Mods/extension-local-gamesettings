    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "bluebird": "^3.4.6",
    "copyfiles": "^2.1.1",
    "i18next": "^3.5.1",
    "immutability-helper": "^2.2.2",
    "json-loader": "^0.5.4",
    "react": "^16.14.0",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^16.14.0",
